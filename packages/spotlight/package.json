{
  "name": "@spotlightjs/spotlight",
  "description": "Spotlight - Sentry for development. Containing the UI, sidecar, and electron app.",
  "version": "4.10.0",
  "productName": "Spotlight",
  "author": "Sentry",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/getsentry/spotlight.git",
    "directory": "packages/spotlight"
  },
  "scripts": {
    "start": "./dist/run.js",
    "dev": "concurrently \"node --disable-warning=ExperimentalWarning --experimental-transform-types --watch src/run.ts\" \"vite --config vite.dev.config.ts\"",
    "dev:ui": "vite --config vite.dev.config.ts",
    "dev:server": "node --disable-warning=ExperimentalWarning --experimental-transform-types --watch src/run.ts",
    "dev:electron": "vite --config vite.electron.config.ts",
    "build": "vite build --config vite.node.config.ts && vite build --config vite.ui.config.ts && npm run build:sea && npm run build:electron",
    "build:sea": "fossilize -m dist/ui/manifest.json -o spotlight dist/run.js",
    "build:watch": "vite build --watch",
    "build:electron": "vite build --config vite.electron.config.ts",
    "build:mac": "./electron-builder.cjs",
    "preview": "vite preview",
    "lint:react-refresh": "eslint 'src/ui/**/*.{ts,tsx}'",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:e2e": "pnpm run test:e2e:cli && pnpm run test:e2e:ui && pnpm run test:e2e:electron",
    "test:e2e:cli": "vitest run --config vitest.cli.config.ts",
    "test:e2e:ui": "playwright test tests/e2e/ui",
    "test:e2e:electron": "playwright test tests/electron.test.ts",
    "sample": "node ./_fixtures/send_to_sidecar.cjs"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "spotlight": "./dist/run.js"
  },
  "main": "./dist/server/main.js",
  "module": "./dist/server/main.js",
  "types": "./dist/server/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/server/main.js",
      "types": "./dist/server/main.d.ts"
    },
    "./sdk": {
      "import": "./dist/server/sdk.js",
      "types": "./dist/server/sdk.d.ts"
    }
  },
  "dependencies": {
    "@hono/mcp": "^0.2.2",
    "@hono/node-server": "^1.19.3",
    "@jridgewell/trace-mapping": "^0.3.25",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@sentry/core": "catalog:",
    "@sentry/node": "catalog:",
    "anser": "^2.3.3",
    "chalk": "^5.6.2",
    "eventsource": "^4.0.0",
    "fast-fuzzy": "^1.12.0",
    "hono": "^4.11.4",
    "launch-editor": "^2.9.1",
    "logfmt": "^1.4.0",
    "mcp-proxy": "^5.6.0",
    "semver": "^7.7.3",
    "uuidv7": "^1.0.2",
    "yaml": "^2.8.1",
    "zod": "^4"
  },
  "devDependencies": {
    "@electron/notarize": "^2.5.0",
    "@fontsource/raleway": "catalog:",
    "@microlink/react-json-view": "^1.23.4",
    "@playwright/test": "^1.56.1",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/browser": "^10.31.0",
    "@sentry/electron": "^7.5.0",
    "@sentry/react": "^10.31.0",
    "@sentry/vite-plugin": "^2.22.5",
    "@shikijs/transformers": "^3.13.0",
    "@tailwindcss/vite": "catalog:",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/beautify": "^0.0.3",
    "@types/logfmt": "^1.2.6",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.20",
    "beautify": "^0.0.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "electron": "^35.7.5",
    "electron-builder": "^24.13.3",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.6.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "fetch-event-stream": "^0.1.5",
    "fossilize": "^0.4.2",
    "happy-dom": "^20.0.2",
    "magic-string": "^0.30.11",
    "nanovis": "^0.1.3",
    "platformicons": "^7.0.4",
    "react": "catalog:",
    "react-diff-viewer-continued": "^3.4.0",
    "react-dom": "catalog:",
    "react-router-dom": "^6.30.2",
    "rollup-plugin-sourcemaps2": "^0.4.1",
    "shiki": "^3.13.0",
    "sql-formatter": "^12.2.4",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "catalog:",
    "tsc-alias": "^1.8.16",
    "tw-animate-css": "^1.3.4",
    "typescript": "catalog:",
    "usehooks-ts": "^2.16.0",
    "vite": "catalog:",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-svgr": "^3.3.0",
    "vitest": "catalog:",
    "zustand": "^5.0.3"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "engines": {
    "node": ">=20"
  }
}
