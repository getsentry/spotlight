{
  "name": "@spotlightjs/spotlight",
  "description": "Spotlight - Sentry for development. Containing the UI, sidecar, and electron app.",
  "version": "4.4.0",
  "productName": "Spotlight",
  "author": "Sentry",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/getsentry/spotlight.git",
    "directory": "packages/spotlight"
  },
  "scripts": {
    "start": "./bin/run.js",
    "dev": "vite serve --config=vite.dev.config.ts",
    "dev:electron": "electron-vite dev",
    "build": "NODE_ENV=production tsc --skipLibCheck && vite build && vite build --config vite.ui.config.ts && npm run build:sea",
    "build:sea": "fossilize -m dist/ui/manifest.json",
    "build:watch": "vite build --watch",
    "build:electron": "electron-vite build",
    "build:mac": "./electron-builder.js",
    "preview": "vite preview",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:e2e": "playwright test",
    "yalc:publish": "yalc publish --push --sig --private",
    "clean": "rimraf dist out"
  },
  "files": ["dist", "bin", "out"],
  "bin": {
    "spotlight": "./bin/run.js",
    "spotlight-sidecar": "dist/sidecar/run.js"
  },
  "main": "./dist/sentry-spotlight.umd.cjs",
  "module": "./dist/sentry-spotlight.js",
  "types": "./dist/sentry-spotlight.d.ts",
  "exports": {
    ".": {
      "types": "./dist/sentry-spotlight.d.ts",
      "import": "./dist/sentry-spotlight.js",
      "require": "./dist/sentry-spotlight.umd.cjs"
    },
    "./sidecar": {
      "import": "./dist/sidecar/main.js",
      "types": "./dist/sidecar/main.d.ts"
    },
    "./sidecar/cli": {
      "import": "./dist/sidecar/cli.js",
      "types": "./dist/sidecar/cli.d.ts"
    },
    "./sidecar/constants": {
      "import": "./dist/sidecar/constants.js",
      "types": "./dist/sidecar/constants.d.ts"
    },
    "./sidecar/parser": {
      "import": "./dist/sidecar/parser/index.js",
      "types": "./dist/sidecar/parser/index.d.ts"
    },
    "./sidecar/format": {
      "import": "./dist/sidecar/formatters/index.js",
      "types": "./dist/sidecar/formatters/index.d.ts"
    },
    "./sidecar/formatters/md/errors": {
      "import": "./dist/sidecar/formatters/md/errors.js",
      "types": "./dist/sidecar/formatters/md/errors.d.ts"
    },
    "./sidecar/formatters/md/event": {
      "import": "./dist/sidecar/formatters/md/event.js",
      "types": "./dist/sidecar/formatters/md/event.d.ts"
    }
  },
  "dependencies": {
    "@hono/mcp": "^0.1.5",
    "@hono/node-server": "^1.19.3",
    "@jridgewell/trace-mapping": "^0.3.25",
    "@modelcontextprotocol/sdk": "^1.16.0",
    "@sentry/core": "catalog:",
    "@sentry/electron": "^5.6.0",
    "@sentry/node": "catalog:",
    "chalk": "^5.6.2",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.3.9",
    "eventsource": "^4.0.0",
    "fast-fuzzy": "^1.12.0",
    "hono": "^4.10.3",
    "import-meta-resolve": "^4.1.0",
    "launch-editor": "^2.9.1",
    "logfmt": "^1.4.0",
    "mcp-proxy": "^5.6.0",
    "uuidv7": "^1.0.2",
    "zod": "^3"
  },
  "devDependencies": {
    "@electron/notarize": "^2.5.0",
    "@fontsource/raleway": "catalog:",
    "@microlink/react-json-view": "^1.23.4",
    "@playwright/test": "^1.54.2",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/browser": "^10",
    "@sentry/react": "^10",
    "@sentry/vite-plugin": "^2.22.5",
    "@shikijs/transformers": "^3.13.0",
    "@spotlightjs/tsconfig": "workspace:*",
    "@tailwindcss/vite": "catalog:",
    "@types/beautify": "^0.0.3",
    "@types/logfmt": "^1.2.6",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.20",
    "beautify": "^0.0.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "electron": "^35.7.5",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "fetch-event-stream": "^0.1.5",
    "fossilize": "^0.4.2",
    "happy-dom": "^20.0.2",
    "magic-string": "^0.30.11",
    "nanovis": "^0.1.3",
    "platformicons": "^7.0.4",
    "react": "catalog:",
    "react-diff-viewer-continued": "^3.4.0",
    "react-dom": "catalog:",
    "react-router-dom": "^6.26.2",
    "rollup-plugin-sourcemaps2": "^0.4.1",
    "shiki": "^3.13.0",
    "sql-formatter": "^12.2.4",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "catalog:",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.4",
    "typescript": "catalog:",
    "usehooks-ts": "^2.16.0",
    "vite": "catalog:",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-svgr": "^3.3.0",
    "vitest": "catalog:",
    "zustand": "^5.0.3"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "engines": {
    "node": ">=20"
  }
}
